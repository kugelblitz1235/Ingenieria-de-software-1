!StoreClientShoppingWindowModel methodsFor: 'initialization' stamp: 'BAG 7/6/2021 22:48:20'!
initializeWith: aUsername password: aPassword for: aCartID 
	username := aUsername.
	password := aPassword.
	cartID := aCartID.
	cartItems := OrderedCollection new.
	addQuantity := 0.
	removeQuantity := 0.
	cartItemIndex := 1.
	catalogItemIndex := 1.
	restInterface := StoreRestInterface new.
	
	catalog := restInterface sendCatalogRequest.
	
	! !


!StoreClientShoppingWindowModel methodsFor: 'TextModelMorphSelector' stamp: 'BAG 7/2/2021 01:07:10'!
addQuantity
	^addQuantity printString! !

!StoreClientShoppingWindowModel methodsFor: 'TextModelMorphSelector' stamp: 'BAG 7/6/2021 00:43:19'!
addQuantity: aQuantity
	[addQuantity := (aQuantity asNumber) ] 
	on: Error - MessageNotUnderstood  
	do:[:anError |
		addQuantity:=0.
		]. 
	! !

!StoreClientShoppingWindowModel methodsFor: 'TextModelMorphSelector' stamp: 'BAG 7/2/2021 00:00:23'!
cartID
	^cartID! !

!StoreClientShoppingWindowModel methodsFor: 'TextModelMorphSelector' stamp: 'BAG 7/2/2021 01:07:27'!
