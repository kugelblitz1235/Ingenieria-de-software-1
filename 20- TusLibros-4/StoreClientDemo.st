	purchaseIndex := anIndex.
	^true.! !

!StoreClientPurchasesWindowModel methodsFor: 'PluggableListSelector' stamp: 'BAG 7/6/2021 22:25:44'!
purchases
	| purchasesItems |
	purchasesItems := OrderedCollection new.
	purchases keysDo: [:isbn | 
		| quantity total |
		quantity := (purchases at: isbn) at:'quantity'.
		total := (purchases at: isbn) at:'total'.
		purchasesItems add: isbn, ' | ',(quantity printString), ' | ',(total printString).].
	
	^purchasesItems	 ! !


!StoreClientPurchasesWindowModel methodsFor: 'initialization' stamp: 'BAG 7/6/2021 22:13:03'!
initializeWith: aUsername password: aPassword purchases: aListOfPurchases
	username := aUsername.
	password:= aPassword.
	purchases := aListOfPurchases .
	purchaseIndex := 1.! !


!StoreClientPurchasesWindowModel methodsFor: 'LabelModelMorphSelector' stamp: 'BAG 7/6/2021 22:26:21'!
total
	| total |
	
	total := 0.
	purchases keysDo:[:isbn | total := total + ((purchases at: isbn) at: 'total') asNumber]. 
	
	^total printString.! !

!StoreClientPurchasesWindowModel methodsFor: 'LabelModelMorphSelector' stamp: 'BAG 7/6/2021 22:21:54'!
