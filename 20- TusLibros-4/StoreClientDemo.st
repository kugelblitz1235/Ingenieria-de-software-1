	[removeQuantity := aQuantity asNumber ] 
	on: Error - MessageNotUnderstood  
	do:[:anError |
		removeQuantity:=0.
		]. 
	
	! !

!StoreClientShoppingWindowModel methodsFor: 'TextModelMorphSelector' stamp: 'BAG 7/1/2021 23:58:19'!
username
	^username
	 ! !

!StoreClientShoppingWindowModel methodsFor: 'TextModelMorphSelector' stamp: 'BAG 7/1/2021 23:58:11'!
username: aUsername
	username := aUsername.
	 ! !


!StoreClientShoppingWindowModel methodsFor: 'PluggableButtonSelector' stamp: 'BAG 7/7/2021 19:12:51'!
addToCart
	| book |
	[
		book := (catalog keys at: catalogItemIndex).
		
		restInterface sendAddToCartRequestWith: cartID book: book quantity: addQuantity.
		
		cartItems := (restInterface sendListCartRequestWith: cartID) asOrderedCollection. 
		self triggerEvent: #cartUpdate with: self.
			
	] on: Error - MessageNotUnderstood 
	 do: [:anError |
		StoreClientErrorWindow openWith: 'No se pudo agregar el libro al carrito' title: 'agregar libro' .
	].! !

!StoreClientShoppingWindowModel methodsFor: 'PluggableButtonSelector' stamp: 'BAG 7/7/2021 19:04:04'!
checkOutCartRequest
	| ticket |
	
	[
		ticket := restInterface sendCheckOutCartRequestWith: cartID.
		self triggerEvent: #close with: self.
		StoreClientTicketWindow openWith: username password: password ticket: ticket.
		
	] on: Error - MessageNotUnderstood 
	 do: [:anError |
		StoreClientErrorWindow openWith: 'Hubo un error al realizar la venta' title: 'Check out' .
	].
	
	
	! !

!StoreClientShoppingWindowModel methodsFor: 'PluggableButtonSelector' stamp: 'BAG 7/6/2021 22:03:04'!
listPurchasesRequest
	| purchases |
	purchases := restInterface sendListPurchasesFor: username password: password.
	StoreClientPurchasesWindow openWith: username password: password purchases: purchases.! !

!StoreClientShoppingWindowModel methodsFor: 'PluggableButtonSelector' stamp: 'BAG 7/7/2021 20:33:03'!
removeFromCart
	| book |
	
	
	[
		book := ( (OrderedCollection withAll:(Set withAll:cartItems)) at: cartItemIndex).
		
		restInterface sendRemoveFromCartRequestWith: cartID book: book quantity: removeQuantity.
		
		cartItems := (restInterface sendListCartRequestWith: cartID) asOrderedCollection. 
		self triggerEvent: #cartUpdate with: self.
		
	] on: Error - MessageNotUnderstood 
	 do: [:anError |
		StoreClientErrorWindow openWith: 'No se pudo remover el libro' title: 'remover libro' .
	].
	! !


!StoreClientShoppingWindowModel methodsFor: 'PluggableListSelector' stamp: 'BAG 7/2/2021 00:45:09'!
cartItemIndex
	^cartItemIndex
	 ! !

!StoreClientShoppingWindowModel methodsFor: 'PluggableListSelector' stamp: 'BAG 7/6/2021 01:21:21'!
cartItemIndex: anIndex
	cartItemIndex := anIndex.
	^true.
	 ! !

!StoreClientShoppingWindowModel methodsFor: 'PluggableListSelector' stamp: 'BAG 7/7/2021 20:35:06'!
cartItems
	| uniqueItems items |
	uniqueItems := OrderedCollection withAll:(Set withAll:cartItems).
	items := OrderedCollection new.
	
	uniqueItems do: [:book | items add: book, ' x ',(cartItems occurrencesOf: book) printString.].
	
	^items
	 ! !

!StoreClientShoppingWindowModel methodsFor: 'PluggableListSelector' stamp: 'BAG 7/2/2021 00:45:20'!
catalogItemIndex
	^catalogItemIndex
	 ! !

!StoreClientShoppingWindowModel methodsFor: 'PluggableListSelector' stamp: 'BAG 7/6/2021 01:21:27'!
catalogItemIndex: anIndex
	catalogItemIndex := anIndex.
	^true.
	 ! !

!StoreClientShoppingWindowModel methodsFor: 'PluggableListSelector' stamp: 'BAG 7/6/2021 01:03:56'!
catalogItems
	| catalogItems |
	catalogItems := OrderedCollection new.
	catalog keysDo: [:isbn | 
		| price title |
		price := (catalog at: isbn) at:'price'.
		title := (catalog at: isbn) at:'title'.
		catalogItems add: isbn, ' | ',title, ' | ',(price printString).].
	
	^catalogItems	 ! !

