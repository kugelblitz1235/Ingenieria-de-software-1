!StoreRestInterface methodsFor: 'initialization' stamp: 'BAG 7/6/2021 00:49:16'!
sendAddToCartRequestWith: aCartID book: aBook quantity: aQuantity
	| fieldDict resp |

	fieldDict := Dictionary newFromPairs: {'amount'. aQuantity.
									'id'. aCartID.
									'book'. aBook.}.		
	
	resp:= WebClient htmlSubmit: (self url,'/addToCart') fields: fieldDict.
	
	resp isSuccess 
		ifTrue:[^(WebUtils jsonDecode: ((resp content) readStream)).] 
		ifFalse:[^self error: resp content].! !

