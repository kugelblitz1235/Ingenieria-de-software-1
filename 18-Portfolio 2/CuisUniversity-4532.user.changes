!methodRemoval: ReceptiveAccount #reportSummary stamp: 'BAG 6/1/2021 23:05:43'!
reportSummary
	| report |
	report := OrderedCollection new.
	transactions do:[:transaction |
		(transaction isKindOf: Deposit) ifTrue: [report add: 'Depósito por ', (transaction value asString),'.'].
		(transaction isKindOf: Withdraw) ifTrue: [report add: 'Extracción por ', (transaction value asString),'.'].
		(transaction isKindOf: TransferOrigin) ifTrue: [report add: 'Salida por transferencia ', (transaction value asString),'.'].
		(transaction isKindOf: TransferDestination) ifTrue: [report add: 'Entrada por transferencia ', (transaction value asString),'.'].
	].
	report add: ('Balance = ', (self balance asString)).
	
	^report.!
