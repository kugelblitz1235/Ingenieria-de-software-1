!DecimalsTests class methodsFor: 'Instantiation' stamp: 'TT 4/12/2021 15:41:06'!
                     test01NewNumberHasCorrectName
	| aNewNumber |
	aNewNumber := DecimalInteger fromInteger: 123.
	Assert that: 123 isEqualTo: aNewNumber asInteger.! !
!DecimalsTests class methodsFor: 'Instantiation' stamp: 'TT 4/15/2021 14:40:43'!
                           test02NewNumberHasCorrectUnit
	| aNewNumber |
	aNewNumber := DecimalInteger fromInteger: 123.
	Assert that: 3 isEqualTo: aNewNumber unitDigit asNumber asInteger.! !
!DecimalsTests class methodsFor: 'Sum' stamp: 'TT 4/15/2021 14:36:59'!
                    test03AUnitPlusZeroIsAUnit
	| number5|
	number5 := DecimalInteger fromInteger: 5.
	Assert that: 5 isEqualTo: (number5+A) asInteger .! !
!DecimalsTests class methodsFor: 'Sum' stamp: 'TT 4/15/2021 14:37:27'!
                 test04ThreePlusFiveIsEight
	| number3 number5|
	number3 := DecimalInteger fromInteger: 3.
	number5 := DecimalInteger fromInteger: 5.
	Assert that: 8 isEqualTo: (number3+number5) asInteger.! !
!DecimalsTests class methodsFor: 'Sum' stamp: 'TT 4/15/2021 14:37:45'!
                         test05CarryIsHandledProperly
	| number41 number59|
	number41 := DecimalInteger fromInteger: 41.
	number59 := DecimalInteger fromInteger: 59.
	Assert that: 100 isEqualTo: (number41+number59) asInteger.! !
!DecimalsTests class methodsFor: 'Product' stamp: 'TT 4/13/2021 15:30:04'!
         test06ZeroTimesANumberIsZero
	Assert that: 0 isEqualTo: (A* (DecimalInteger fromInteger: 294)) asInteger.! !
!DecimalsTests class methodsFor: 'Product' stamp: 'TT 4/13/2021 15:30:15'!
        test07UnitTimesAUnitEqualsAUnit
	Assert that: 8 isEqualTo: ((DecimalInteger fromInteger: 4) * (DecimalInteger fromInteger: 2)) asInteger.! !
!DecimalsTests class methodsFor: 'Product' stamp: 'TT 4/13/2021 15:30:20'!
        test08ProductBetween21And56Equals1176
	Assert that: 1176 isEqualTo: ((DecimalInteger fromInteger: 21) * (DecimalInteger fromInteger: 56)) asInteger.! !
!DecimalsTests class methodsFor: 'Substraction' stamp: 'TT 4/15/2021 14:38:15'!
                        test09AUnitMinusItselfIsZero
	| number9 |
	number9 := DecimalInteger fromInteger: 9.
	Assert that: A isEqualTo: number9 - number9 .! !
!DecimalsTests class methodsFor: 'Substraction' stamp: 'TT 4/15/2021 14:38:24'!
         test10ZeroMinusAUnitGreaterThanZeroThrowsError
	| number9 |
	number9 := DecimalInteger fromInteger: 9.
	[A-number9 ] on: Error do: [ :anError | Assert that: DecimalInteger exceptionNumberIsNotANatural isEqualTo: anError messageText ].! !
!DecimalsTests class methodsFor: 'Substraction' stamp: 'TT 4/15/2021 14:38:55'!
  test11BorrowIsHandledPropery
	| number10 number1 |
	number1 := DecimalInteger fromInteger: 1.
	number10 := DecimalInteger fromInteger: 10.
	Assert that: (DecimalInteger fromInteger: 9) isEqualTo: (number10-number1).! !
!DecimalsTests class methodsFor: 'Division' stamp: 'TT 4/14/2021 13:25:58'!
                         test12CannotDivideByZero
	[B/A] on: Error do: [ 
		:anError |
		Assert that: anError messageText isEqualTo: DecimalInteger exceptionCannotDivideByZero.
	]! !
!DecimalsTests class methodsFor: 'Division' stamp: 'TT 4/15/2021 14:39:12'!
                      test13ANumberDividedByOneIsThatNumber
	| number10 |
	number10 := DecimalInteger fromInteger: 10.
	Assert that: (number10 / B) asInteger isEqualTo: 10.! !
!DecimalsTests class methodsFor: 'Division' stamp: 'TT 4/15/2021 14:39:21'!
                          test13NoRemainderDivisionsWork
	| number10 number2 |
	number10 := DecimalInteger fromInteger: 10.
	number2 := DecimalInteger fromInteger: 2.
	Assert that: (number10 / number2) asInteger isEqualTo: 5.! !
!DecimalsTests class methodsFor: 'Division' stamp: 'TT 4/15/2021 14:39:32'!
         test14RemainderDivisionsWork
	| number10 number3 |
	number10 := DecimalInteger fromInteger: 10.
	number3 := DecimalInteger fromInteger: 3.
	Assert that: (number10 / number3) asInteger isEqualTo: 3.! !
!DecimalsTests class methodsFor: 'Division' stamp: 'TT 4/14/2021 13:46:57'!
           test15DivisorWithMoreThanOneDigitDivisionWorks
	| number150 number35 |
	number150 := DecimalInteger fromInteger: 150.
	number35 := DecimalInteger fromInteger: 35.
	Assert that: (number150 / number35) asInteger isEqualTo: 4.! !
!DecimalsTests class methodsFor: 'Division' stamp: 'TT 4/14/2021 13:49:06'!
                 test16DivisionBetweenBigNumbersWorks
	| number84593 number384 |
	number84593 := DecimalInteger fromInteger: 84593.
	number384 := DecimalInteger fromInteger: 384.
	Assert that: (number84593 / number384) asInteger isEqualTo: 220.! !

