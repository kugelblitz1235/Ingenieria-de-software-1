	parents size = 0 ifTrue: [
		aFinancialAsset assertNoFinancialAssetIsReachableFrom: self
	] ifFalse: [
		parents do: [ :parent | (parent assertNoFinancialAssetIsSharedWith: aFinancialAsset) ]
	].! !

